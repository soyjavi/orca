    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.2.1",
