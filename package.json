    "on-finished": "^2.3.0",
    "ora": "^5.4.0",
    "pretty-error": "^3.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "telegraf": "^4.3.0",
    "vanilla-storage": "^0.3.11"
